  </servlet-mapping>
  
  <servlet>
    <servlet-name>panier</servlet-name>
    <servlet-class>Controleur.PanierS</servlet-class>
   </servlet>
  
  <servlet-mapping>
     <servlet-name>panier</servlet-name>
     <url-pattern>/panier</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>visualiserPanier</servlet-name>
