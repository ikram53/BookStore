<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>SiteVenteLivre</display-name>
  <welcome-file-list>
   <welcome-file>VueAcceuil.jsp</welcome-file>
    </welcome-file-list>
   <servlet>
    <servlet-name>inscription</servlet-name>
    <servlet-class>Controleur.InscriptionS</servlet-class>
   </servlet>
  
  <servlet-mapping>
     <servlet-name>inscription</servlet-name>
     <url-pattern>/inscription</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>connection</servlet-name>
    <servlet-class>Controleur.ConnectionS</servlet-class>
   </servlet>
  
